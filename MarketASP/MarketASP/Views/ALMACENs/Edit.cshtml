@model MarketASP.Models.ALMACEN

@{
    ViewBag.Title = "Actualizar Almacen";
    ViewBag.pTitle = "Actualizar Almacen";
    ViewBag.pageTitle = "Mantenimiento";
}
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.ncode_alma)
                    @Html.Partial("_Almacen")
                }

                <div>
                    @Html.ActionLink(" ", "Index", new { }, new { @class = "btn btn-primary fas fa-hand-point-left", title = "Listado" })
                </div>

            </div>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
