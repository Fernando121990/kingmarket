@model IEnumerable<MarketASP.Models.UBIGEO>

@{
    ViewBag.Title = "Lista de Ubigeo";
}
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h5 class="h3 mb-0 text-gray-800">@ViewBag.Title</h5>
</div>

<table class="table tblGralExporta" style="width:100%">
    <thead class="thead-dark">
        <tr>
            <th>
                Departamento
            </th>
            <th>
                Provincia
            </th>
            <th>
                Distrito
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.sdepa_ubigeo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sprovi_ubigeo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sdistri_ubigeo)
                </td>
            </tr>
        }
    </tbody>
</table>
