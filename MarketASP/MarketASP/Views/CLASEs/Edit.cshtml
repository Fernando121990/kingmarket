@model MarketASP.Models.CLASE

@{
    ViewBag.Title = "Editar Clase";
    ViewBag.pTitle = "Editar Clase";
    ViewBag.pageTitle = "Mantenimiento";
}

<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    @Html.HiddenFor(model => model.ncode_clase)
                    @Html.Partial("_clase")
                }

                <div>
                    @Html.ActionLink(" ", "Index", new { }, new { @class = "btn btn-primary fas fa-hand-point-left", title = "Lista de Clases" })
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
