@model MarketASP.Models.CONFIGURACION

@{
    ViewBag.Title = "Agregar";
    ViewBag.pTitle = "Agregar";
    ViewBag.pageTitle = "Configuración";
}
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                    @Html.Partial("_Configuracion")

                }
                <div>
                    @Html.ActionLink(" ", "Index", new { }, new { @class = "btn btn-primary fas fa-hand-point-left", title = "Lista de Configuración" })
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
