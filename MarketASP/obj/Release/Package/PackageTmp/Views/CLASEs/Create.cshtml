@model MarketASP.Models.CLASE

@{
    ViewBag.Title = "Agregar Clase";
    ViewBag.pTitle = "Agregar Clase";
    ViewBag.pageTitle = "Mantenimiento";
}
<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                <div class="form-horizontal">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                    @Html.Partial("_clase")
                </div>
                }
                <div>
                    @Html.ActionLink(" ", "Index", new { }, new { @class = "btn btn-primary fas fa-hand-point-left", title = "Lista de Clases" })
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
